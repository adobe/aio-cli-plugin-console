{
    "extends": ["@adobe/eslint-config-aio-lib-config"],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "settings": {
      "jsdoc": {
        "ignorePrivate": true
      }
    },
    "rules": {
      "jsdoc/tag-lines": [
          // The Error level should be `error`, `warn`, or `off` (or 2, 1, or 0)
          "error",
          "never",
          {
            "startLines": null
          }
      ]
    }
  }