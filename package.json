{
  "name": "@adobe/aio-cli-plugin-console",
  "description": "Adobe I/O Console plugin for the Adobe I/O CLI",
  "version": "2.0.4",
  "author": "Adobe Inc.",
  "bugs": "https://github.com/adobe/aio-cli-plugin-console/issues",
  "dependencies": {
    "@adobe/aio-lib-core-config": "^1.0.15",
    "@oclif/command": "1.5.20",
    "@oclif/config": "1.13.3",
    "@oclif/errors": "^1.1.2"
  },
  "devDependencies": {
    "@adobe/eslint-config-aio-lib-config": "^1.2.0",
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2.1.4",
    "@oclif/test": "^1",
    "chalk": "^4.0.0",
    "codecov": "^3.2.0",
    "eslint": "^6.0.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.11.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "execa": "^4.0.1",
    "jest": "^24.9.0",
    "jest-haste-map": "^26.0.0",
    "jest-junit": "^10.0.0",
    "stdout-stderr": "^0.1.9"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "src/index.js",
  "files": [
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/adobe/aio-cli-plugin-console",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "Apache-2.0",
  "oclif": {
    "commands": "./src/commands",
    "bin": "./bin/run",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "console": {
        "description": "Adobe I/O Console API the Adobe I/O Runtime"
      }
    },
    "repositoryPrefix": "<%- repo %>/blob/<%- version %>/<%- commandPath %>"
  },
  "repository": "adobe/aio-cli-plugin-console",
  "scripts": {
    "lint": "eslint src test e2e",
    "pretest": "npm run lint",
    "test": "npm run unit-tests",
    "unit-tests": "jest --ci -w=2",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "version": "oclif-dev readme && git add README.md",
    "e2e": "jest --collectCoverage=false --testRegex './e2e/e2e.js'"
  }
}
